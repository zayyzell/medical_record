<h1>Sign up</h1>
  <%= form_for @user do |f| %>
<p>
    <%= f.label :email %>
    <%= f.text_field :email %>
</p>
<p>
    <%= f.label :password %>
    <%= f.password_field :password %>
</p>
<p>
    <%= f.label :password_confirmation %>
    <%= f.password_field :password_confirmation %>
</p>
<p>
    <%= f.submit %>
</p>
  <% end %>
<%= link_to 'Login', new_sessions_path %>
